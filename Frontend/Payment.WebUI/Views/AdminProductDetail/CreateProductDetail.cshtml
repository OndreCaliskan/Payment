@model CreateProductDetailDto
@{
    ViewData["Title"] = "CreateProductDetail";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<h1>Product Detail Ekleme Sayfası</h1>

<form method="post">
    <label>Description</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    <label>ProductInfo</label>
    <input type="text" asp-for="ProductInfo" class="form-control" />
    <br />
    <label>ProductID</label>
    <input type="text" asp-for="ProductID" class="form-control" />
    <br />
    <button class="btn btn-primary">Product Detail Ekle</button>
</form>
